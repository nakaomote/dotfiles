#!/bin/bash

set -eu

for brewPkg in \
    ack \
    blueutil \
    coreutils \
    ffmpeg \
    gh \
    git \
    go \
    jq \
    neovim \
    node \
    rlwrap \
    tig \
    tree \
    $()
do
    /usr/local/bin/brew list "${brewPkg}" > /dev/null || /usr/local/bin/brew install "${brewPkg}"
done
